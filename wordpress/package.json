{
  "name": "superstack-wp",
  "version": "1.0.0",
  "description": "starter website backend - frontend built on Next.js",
  "main": "index.js",
  "repository": "https://github.com/superhuit-agency/superstack",
  "author": "superhuit <tech@superhuit.ch>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prestart": "sh ./scripts/pre-start.sh",
    "dev": "NEXT_PUBLIC_IS_THIS_NEXT=false NODE_ENV=development webpack server",
    "start": "sh ./scripts/start-wp.sh",
    "stop": "docker compose down",
    "logs": "docker compose logs -f",
    "build": "THEME_NAME=superstack NEXT_PUBLIC_IS_THIS_NEXT=false NODE_ENV=production webpack",
    "generate:pot": "wp i18n make-pot ./theme theme/languages/supt.pot",
    "generate:json": "wp i18n make-json ./theme/languages/fr_FR.po --no-purge",
    "analyze": "ANALYZE=true NODE_ENV=production webpack"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@soda/friendly-errors-webpack-plugin": "^1.8.1",
    "@tsconfig/node22": "^22.0.2",
    "@types/wordpress__block-editor": "^11.5.17",
    "@types/wordpress__block-library": "^2.6.3",
    "@types/wordpress__blocks": "^12.5.18",
    "@types/wordpress__edit-post": "^8.4.2",
    "@types/wordpress__plugins": "^6.0.0",
    "@types/wordpress__rich-text": "^6.10.0",
    "@wordpress/api-fetch": "^7.27.0",
    "@wordpress/blob": "^4.27.0",
    "@wordpress/plugins": "^7.27.0",
    "@wordpress/url": "^4.27.0",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "crypto-browserify": "^3.12.1",
    "css-loader": "^7.1.2",
    "image-minimizer-webpack-plugin": "^4.1.3",
    "postcss-loader": "^8.1.1",
    "process": "^0.11.10",
    "sharp": "^0.34.3",
    "stream-browserify": "^3.0.0",
    "style-loader": "^4.0.0",
    "typescript": "^5.8.3",
    "webpack": "^5.101.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-manifest-plugin": "^5.0.1"
  }
}
