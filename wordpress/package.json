{
  "name": "superstack-wp",
  "version": "1.0.0",
  "description": "starter website backend - frontend built on Next.js",
  "main": "index.js",
  "repository": "https://github.com/superhuit-agency/superstack",
  "author": "superhuit <tech@superhuit.ch>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prestart": "sh ./scripts/pre-start.sh",
    "dev": "NEXT_PUBLIC_IS_THIS_NEXT=false NODE_ENV=development webpack server",
    "start": "sh ./scripts/start-wp.sh",
    "stop": "docker compose down",
    "logs": "docker compose logs -f",
    "build": "NEXT_PUBLIC_IS_THIS_NEXT=false NODE_ENV=production webpack",
    "generate:pot": "wp i18n make-pot ./theme theme/languages/supt.pot",
    "generate:json": "wp i18n make-json ./theme/languages/fr_FR.po --no-purge",
    "analyze": "ANALYZE=true NODE_ENV=production webpack"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@soda/friendly-errors-webpack-plugin": "^1.8.1",
    "@tsconfig/node20": "^20.1.2",
    "@types/wordpress__block-editor": "^11.5.0",
    "@types/wordpress__block-library": "^2.6.1",
    "@types/wordpress__blocks": "^12.5.0",
    "@types/wordpress__edit-post": "^7.0.2",
    "@types/wordpress__plugins": "^3.0.0",
    "@types/wordpress__rich-text": "^6.4.0",
    "@wordpress/api-fetch": "^7.4.0",
    "@wordpress/blob": "^4.4.0",
    "@wordpress/plugins": "^7.4.0",
    "@wordpress/url": "^4.4.0",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.7.3",
    "image-minimizer-webpack-plugin": "^3.8.2",
    "postcss-loader": "^8.1.0",
    "process": "^0.11.10",
    "resolve-url-loader": "^5.0.0",
    "sharp": "^0.32.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.2",
    "typescript": "^5.0.4",
    "webpack": "^5.79.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.13.3",
    "webpack-manifest-plugin": "^5.0.0"
  }
}
